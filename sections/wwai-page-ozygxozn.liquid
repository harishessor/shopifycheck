function setupSmoothScrollToBottom(targetSectionId, durationMs = 1000) {
 const triggers = document.querySelectorAll('[data-wwai-btn-review-navigate]');
 const target = document.getElementById(targetSectionId);


 if (!target || !triggers.length) {
   console.warn('[WWAI] Missing trigger(s) or target section.');
   return;
 }


 console.log('REVIEW: triggers', triggers, target);


 triggers.forEach(trigger => {
   trigger.style.cursor = 'pointer';


   trigger.addEventListener('click', function (e) {
     e.preventDefault();
     console.log('REVIEW: click');


     const targetPosition = target.getBoundingClientRect().top + window.pageYOffset + target.offsetHeight;
     const startPosition = window.pageYOffset;
     const distance = targetPosition - startPosition;
     const duration = durationMs;
     let start = null;


     function easeFastStart(p) {
       return p < 0.0005
         ? 8 * p  // Almost immediate ramp-up (linear)
         : -1 + (4 - 2 * p) * p;
     }


     function animation(currentTime) {
       if (start === null) start = currentTime;
       const timeElapsed = currentTime - start;
       const progress = Math.min(timeElapsed / duration, 1);
       const ease = easeFastStart;


       window.scrollTo(0, startPosition + distance * ease(progress));


       if (timeElapsed < duration) {
         requestAnimationFrame(animation);
       }
     }


     requestAnimationFrame(animation);
   });
 });
}


// ⏳ Retry every 200ms until elements are ready (max 5s)
function waitForReviewElements(targetId, maxWaitMs = 5000) {
 const interval = 200;
 let waited = 0;


